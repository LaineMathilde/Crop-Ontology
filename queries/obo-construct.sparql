PREFIX oboInOwl: <http://www.geneontology.org/formats/oboInOwl#>
prefix dc:    <http://purl.org/dc/elements/1.1/> 
prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> 
prefix obo:   <http://purl.obolibrary.org/obo/> 
prefix skosxl: <http://www.w3.org/2008/05/skos-xl#> 
prefix foaf:  <http://xmlns.com/foaf/0.1/> 
prefix dwc:   <http://rs.tdwg.org/dwc/terms/> 
prefix dct:   <http://purl.org/dc/terms/> 
prefix cov:   <http://www.cropontology.org/vocab/> 
prefix xsd:   <http://www.w3.org/2001/XMLSchema#> 
prefix owl:   <http://www.w3.org/2002/07/owl#> 
prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
prefix oboInOwl: <http://www.geneontology.org/formats/oboInOwl#> 
prefix skos:  <http://www.w3.org/2004/02/skos/core#> 

SELECT ?uri ?subClassOf ?methodOf ?scaleOf ?label ?skosDefinition
WHERE {

{ ?uri a owl:Class . }
UNION
{ ?uri a owl:Ontology . }

OPTIONAL { ?uri rdfs:label ?label }

OPTIONAL { ?uri skos:altLabel ?synonym .}
OPTIONAL { ?uri cov:acronym ?relatedSyonym .}
OPTIONAL { ?uri skos:definition ?skosDefinition .}

OPTIONAL { ?uri rdfs:subClassOf ?subClassOf . }
OPTIONAL { ?uri cov:methodOf ?methodOf . }
OPTIONAL { ?uri cov:scaleOf ?scaleOf . }


}
